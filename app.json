{
  "expo": {
    "name": "Gardrops",
    "slug": "gardrops",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "gardrops",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "backgroundColor": "#605a64"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cankiraz.gardrops"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gardrops.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Uygulamanın kameranıza erişmesi gerekiyor."
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#605a64",
          "image": "./assets/splash.png",
          "resizeMode": "contain"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "abe2caa1-ca4d-4434-a8ff-f131fa3334f3"
      }
    }
  }
}
