{
  "expo": {
    "name": "Gardrops",
    "slug": "gardrops",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "gardrops",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A3F69"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cankiraz.gardrops"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#4A3F69"
      },
      "package": "com.cankiraz.gardrops",
      "versionCode": 1
    },
    "plugins": [
      "expo-router"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "abe2caa1-ca4d-4434-a8ff-f131fa3334f3"
      }
    }
  }
}
